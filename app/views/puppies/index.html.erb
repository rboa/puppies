<h1>Listing puppies</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Name</th>
    <th>Major</th>
    <th>Minor</th>
    <th>Stuff</th>
  </tr>

<% @puppies.each do |puppy| %>
  <tr>
    <td><%= puppy.url %></td>
    <td><%= puppy.name %></td>
    <td><%= puppy.major %></td>
    <td><%= puppy.minor %></td>
    <td><%= link_to 'Show', puppy %></td>
    <td><%= link_to 'Edit', edit_puppy_path(puppy) %></td>
    <td><%= link_to 'Destroy', puppy, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Puppy', new_puppy_path %>

<br />
<%= create_heroes_and_marines(@puppies[0..3]) %>
